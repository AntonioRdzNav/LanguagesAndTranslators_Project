
# NOTE: CORRECT SYNTAX

PROGRAM Functions_TEST_CORRECT

# Global variables declaration
DIM a, b AS WORD

SUB PROCEDURE sumNumbers:
  a = a + b
  RETURN

SUB PROCEDURE interactWithUser:
  CLS
  b = INPUT("Please enter a value for b")
  GOSUB sumNumbers
  PRINT("a = ", a, "...")
  RETURN

MAIN:

  GOSUB interactWithUser

END .